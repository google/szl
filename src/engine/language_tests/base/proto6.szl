# Minimal test of the -print_referenced_tuple_field_names option.
# More complex tests can be found in unreachable/unused_fields.szl.

type U = {
  m: int @ 1,
  n: array of U @ 2
};

type T = {
  x: int @ 1,
  y: array of T @ 2,
  z: { a: int @ 3 } @ 4,
  w: array of { b: int @ 5 } @ 6,
  u: U @ 7
};

t: T = T(input);  # no values, but needed to determine primary proto type

t = { 1, { }, { 2 }, { { 3 } }, { 4, { } } };

t.x;
c: int = t.z.a;
a: int = t.w[0].b;
d: int = t.u.m;
