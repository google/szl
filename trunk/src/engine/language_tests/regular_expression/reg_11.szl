#!/bin/env szl
#szl_options

#desc: Character classes, special characters and escape sequences.

s0: string;
matchArray: array of string;

#inst: \Q...\E escape chars.
s0 = "$^[()]";

matchArray = matchstrs(`\Q$^[()]\E`, s0);
if (matchArray[0] != "$^[()]")
{
    emit stdout <- format(`matchstrs("\Q$^[()]\E", "%s") Failed !!!`, s0);
}
