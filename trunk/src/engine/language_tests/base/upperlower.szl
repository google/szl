# This program produces the following output, identifying
# the characters that change the size of the UTF-8 representation
# between upper and lower case:

# We include the running of this program in the test so that if the
# Unicode standard changes, the golden test will fail and we will
# update the test.
expect: map[int] of string = {
  304   : "130: İ: i(1) İ(1)",
  305   : "131: ı: ı(1) I(1)",
  383   : "17f: ſ: ſ(1) S(1)",
  570   : "23a: Ⱥ: ⱥ(1) Ⱥ(1)",
  574   : "23e: Ⱦ: ⱦ(1) Ⱦ(1)",
  619   : "26b: ɫ: ɫ(1) Ɫ(1)",
  637   : "27d: ɽ: ɽ(1) Ɽ(1)",
  8126  : "1fbe: ι: ι(1) Ι(1)",
  8486  : "2126: Ω: ω(1) Ω(1)",
  8490  : "212a: K: k(1) K(1)",
  8491  : "212b: Å: å(1) Å(1)",
  11362 : "2c62: Ɫ: ɫ(1) Ɫ(1)",
  11364 : "2c64: Ɽ: ɽ(1) Ɽ(1)",
  11365 : "2c65: ⱥ: ⱥ(1) Ⱥ(1)",
  11366 : "2c66: ⱦ: ⱦ(1) Ⱦ(1)"
};

for (i: int = 1; i < 65536; i++) {
  if (0xD800 <= i && i <= 0xDFFF)  # surrogate
    continue;
  char: string = new(string, 1, i);
  lower: string = lowercase(char);
  upper: string = uppercase(char);
  if (len(bytes(upper)) != len(bytes(lower)))
    assert(expect[i] == format("%x: %c: %s(%d) %s(%d)", i, i,
                               lowercase(char), len(lower),
                               uppercase(char), len(upper)));
}

x: int;
# simple stuff
assert(lowercase("HelloÄ日本語ΛЖ") == "helloä日本語λж");
assert(uppercase("helloä日本語λж") == "HELLOÄ日本語ΛЖ");


